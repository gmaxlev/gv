RewriteCond %{ENV:HTTPS} !on
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

RewriteCond %{HTTP_HOST} ^www\.(.*)$
RewriteRule ^(.*)$ http://%1/$1 [L,R=301] 

Redirect 301 https://gv-kuhni.com.ua/index.php https://gv-kuhni.com.ua
Redirect 301 https://gv-kuhni.com.ua/index.html https://gv-kuhni.com.ua
Redirect 301 https://gv-kuhni.com.ua/?Id=7777 https://gv-kuhni.com.ua 

# WEEK
<FilesMatch ".(jpg|jpeg|png|gif|swf)$">
	Header set Cache-Control "max-age=604800, public"
</FilesMatch>

# WEEK
<FilesMatch ".(js|css|swf)$">
	Header set Cache-Control "max-age=604800"
</FilesMatch>

#убираем лишние слеши
RewriteCond %{REQUEST_URI} ^(.*?)\/{2,}(.*?)$
RewriteRule . %1/%2 [L,R=404]
RewriteCond %{THE_REQUEST} //
RewriteRule .* /$0 [R=404,L]